  "devDependencies": {
    "@serverless/typescript": "^2.23.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/cors": "^2.8.10",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.5",
    "esbuild": "^0.12.15",
    "jest": "^27.0.6",
    "json-schema-to-ts": "^1.5.0",
